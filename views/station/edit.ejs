<% include ../partials/header %>

<form action="/station/<%=station._id%>?_method=PUT"  method="POST" >
	<div class="container" style="width: 33%;">
		<input type="hidden" name="oldStationId" value="<%= station.stationId%>">
		<div class="form-group" >
			<label for="station[stationName]" >Station Name:</label>
			<input class= "form-control" type="text" name="station[stationName]" value="<%= station.stationName%>">
		</div>
		<div class="form-group" >
			<label for="station[stationId]" >Station Id</label>
			<input class= "form-control" type="text" name="station[stationId]" value="<%= station.stationId%>">
		</div>
		<div class="form-group">
			<button class="btn btn-primary form-control">Submit</button>
		</div>
	</div>
</form>

<% include ../partials/footer %>

