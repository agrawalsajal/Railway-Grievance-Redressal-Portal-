<% include ../partials/header %>

<form action="/dept/<%= dept._id%>?_method=PUT"  method="POST" >
	<div class="container" style="width: 33%;">
		<input type="hidden" name="oldDeptName" value="<%= dept.deptName %>" >
		<div class="form-group" >
			<label for="dept[deptName]" >Department Name:</label>
			<input class= "form-control" type="text" name="dept[deptName]" value="<%= dept.deptName%>">
		</div>
		<div class="form-group" >
			<label for="dept[deptId]" >Department Id:</label>
			<input class= "form-control" type="text" name="dept[deptId]" value="<%= dept.deptId%>">
		</div>
		<div class="form-group">
			<button class="btn btn-primary form-control">Submit</button>
		</div>
	</div>
</form>

<% include ../partials/footer %>
